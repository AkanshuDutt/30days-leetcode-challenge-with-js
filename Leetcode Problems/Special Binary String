var makeLargestSpecial = function(s) {
    let count = 0, i = 0;
    let res = [];
    for (let j = 0; j < s.length; j++) {
        count += s[j] === '1' ? 1 : -1;
        if (count === 0) {
            res.push("1" + makeLargestSpecial(s.substring(i + 1, j)) + "0");
            i = j + 1;
        }
    }
    res.sort().reverse();
    return res.join("");
};
